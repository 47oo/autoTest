	<!-- 数据的name,只需要name -->
	<keys>
		<key name="hh">
			<key name="test"></key>
		</key>
		<key name="bb">
			<key name="love">
				<key name="for"></key>
				<key name="ever"></key>
			</key>
			<key name="too"></key>
		</key>
	</keys>
</conf>